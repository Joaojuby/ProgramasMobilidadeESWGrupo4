<Window x:Class="WpfApp2.Paises"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner"
      Title="Programas Mobilidade ESW - Grupo 4 - Editar Países"
        Height="400" Width="600"
        MaxHeight="400" MaxWidth="600"
        MinHeight="400" MinWidth="600" >
    <DockPanel HorizontalAlignment="Stretch" Name="dockPanel1" VerticalAlignment="Top">
        <Button Width="40" Height="40" HorizontalAlignment="Left" BorderThickness="3" BorderBrush="DeepSkyBlue"  Name="AButton"  Click="AButton_Click" RenderTransformOrigin="0.651,0.502">
            <Image Source="data\home.png"></Image>
        </Button>
        <Button Width="40" Height="40" HorizontalAlignment="Left" BorderThickness="3" BorderBrush="Black"  Name="Button"  Click="AButton_Click" RenderTransformOrigin="0.651,0.502">
            <Image Source="data\save.png"></Image>
        </Button>
    </DockPanel>
</Window>